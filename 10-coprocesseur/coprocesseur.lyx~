\begin_layout BeginFrame
L'encodage des réels sous Intel 8087
\end_layout

\begin_layout Itemize
Encodage des réels en format 
\begin_inset Quotes fld
\end_inset


\emph on
x86 Extended Precision
\emph default

\begin_inset Quotes frd
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Bit de signe 
\begin_inset Formula $s$
\end_inset


\end_layout

\begin_layout Itemize
Exposant 
\begin_inset Formula $e$
\end_inset

 sur 15 bits (avec biais de 16383)
\end_layout

\begin_layout Itemize
Mantisse 
\begin_inset Formula $m$
\end_inset

 sur 64 bits (partie entière en bit 63, partie fractionnaire au-delà)
\end_layout

\end_deeper
\begin_layout Standard
\align center
\begin_inset Graphics
	filename images/762px-X86_Extended_Floating_Point_Format.svg.png
	width 80page%

\end_inset


\end_layout

\begin_layout Itemize
Formule:
\begin_inset Formula 
\[
(-1)^{s}\times m\times2^{e-16383}
\]

\end_inset


\end_layout

\begin_layout BeginFrame
L'encodage des réels sous Intel 8087
\end_layout

\begin_layout Itemize
Exemple: 
\begin_inset Formula $4001C000000000000000$
\end_inset

 
\end_layout

\begin_layout Standard
\align center
\begin_inset Graphics
	filename images/exemple-encodage.jpeg
	width 75page%

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $s=0$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $e=100000000000001_{2}$
\end_inset


\begin_inset Formula $=16385_{10}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $m=1.100000....0_{2}=1.5_{10}$
\end_inset


\end_layout

\begin_layout Itemize
Formule: 
\begin_inset Formula 
\begin{eqnarray*}
(-1)^{s}\times m\times2^{e-16383}\, & = & \,(-1)^{0}\times1.5\times2^{16385-16383}\\
 & = & 1\times1.5\times2^{2}\\
 & = & 6
\end{eqnarray*}

\end_inset

 
\end_layout

